import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		double[] arr = new double[3];
		for (int i = 0; i < 3; i++){
		    arr[i] = scan.nextDouble();
		}
		for (double u : arr) {
		    System.out.println(u + "  |  " + (u >= 0 ? Math.pow(u, 2) : Math.pow(u, 4)));
		}
	}
}
