import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		int a = scan.nextInt();
		int b = scan.nextInt();
		int c = scan.nextInt();
		int max = a > b ? a : b;
		max = max > c ? max : c;
		int min = a < b ? a : b;
		min = min < c ? min : c;
		System.out.printf("Наибольшее число - %d\nНаименьшее число - %d", max, min);
	}
}
